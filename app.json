{
  "expo": {
    "name": "soSmall Weather App",
    "slug": "sosmall_weather",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soSmall.soSmallWeatherApp"
    },
    "android": {
      "package": "com.soSmall.soSmallWeatherApp",
      "versionCode": 2 
    },
    "description": "This is a cute weather app that tells your local weather! Made using React Native and Expo."
  }
}